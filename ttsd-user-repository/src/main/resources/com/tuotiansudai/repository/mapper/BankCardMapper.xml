<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuotiansudai.repository.mapper.BankCardMapper">

    <select id="findPassedBankCardNumberByLoginName" parameterType="java.lang.String" resultType="string">
        select card_number from bank_card where login_name = #{loginName} and status = 'PASSED'
    </select>
</mapper>