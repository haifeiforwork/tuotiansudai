<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuotiansudai.repository.mapper.UserInfoLogMapper">

    <insert id="create" parameterType="UserInfoLogModel">
        insert into user_info_log (id,operate_time,login_name,ip,description)
        value(#{id}, #{operateTime}, #{loginName},#{ip},#{description})
    </insert>
</mapper>