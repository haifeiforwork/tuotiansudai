<#include "header.ftl" />
<div id="nav"><!--nav begin-->
    <div class="nav_main">
        <a href="homepage.html" class="logo">
            <img src="${requestContext.getContextPath()}/images/logo.png" alt="">
        </a>
        <p>注册</p>
        <span>已有账号，请<a href="javascript:;">登陆</a></span>
    </div>
</div>
