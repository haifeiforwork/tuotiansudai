<table class="invest-list table-striped">
    <thead>
    <tr>
        <th width="130px">项目名称</th>
        <th width="120px">所用券</th>
        <th>券额(元)</th>
        <th>使用时间</th>
        <th>预计收益(元)</th>
    </tr>
    </thead>
    <tbody>
    {{#records}}
    <tr>
        <td><a href="/loan/{{loanId}}" class="project-name">{{loanName}}</td>
        <td>{{couponName}}</td>
        <td>{{couponAmountStr}}</td>
        <td>{{usedTime}}</td>
        <td>{{expectedIncomeStr}}</td>
    </tr>
    {{/records}}
    {{^records}}
    <td colspan="5" class="no-data">暂时没有使用记录</td>
    {{/records}}
    </tbody>
</table>